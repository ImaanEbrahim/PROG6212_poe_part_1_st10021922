<Window x:Class="CMCSAPP_PROTOTYPE.UploadDocumentsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Upload Documents" Height="200" Width="400">
    <Grid>
        <StackPanel Margin="10">
            <TextBlock Text="Choose a document to upload:" />
            <Button Content="Browse" Click="BrowseButton_Click" Margin="5"/>
            <Button Content="Upload" Click="UploadButton_Click" Margin="5"/>
        </StackPanel>
    </Grid>
</Window>
